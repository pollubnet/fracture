	private List<Item> equipment = new();
	private List<Item> inventory = new();

	Dictionary<string, object> equipmentPopupParameters = new();

	private PopupContainer popup;
	protected override Task OnInitializedAsync()
	{
		equipmentPopupParameters.Add("inventory", inventory);
		equipmentPopupParameters.Add("equipment", equipment);
		equipmentPopupParameters.Add("OnEquipmentChanged", EventCallback.Factory.Create(this, HandleEquipmentChanged));
		return base.OnInitializedAsync();
	}

	public async Task HandleEquipmentChanged()
	{
		await InvokeAsync(StateHasChanged);
	}
}
